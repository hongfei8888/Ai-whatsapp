{
  "name": "whatsapp-web.js",
  "version": "1.34.1",
  "description": "Library for interacting with the WhatsApp Web API ",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "test": "mocha tests --recursive --timeout 5000",
    "test-single": "mocha",
    "shell": "node --experimental-repl-await ./shell.js",
    "generate-docs": "npx jsdoc --configure .jsdoc.json --verbose",
    "dev": "tsx --require dotenv/config app/src/main.ts",
    "start": "node app/dist/main.js",
    "build": "tsc --project tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:push": "prisma db push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedroslopez/whatsapp-web.js.git"
  },
  "keywords": [
    "whatsapp",
    "whatsapp-web",
    "api",
    "bot",
    "client",
    "node"
  ],
  "author": "Pedro Lopez",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pedroslopez/whatsapp-web.js/issues"
  },
  "homepage": "https://wwebjs.dev/",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@pedroslopez/moduleraid": "^5.0.2",
    "@prisma/client": "^6.16.2",
    "dotenv": "^16.6.1",
    "fastify": "^5.6.1",
    "fluent-ffmpeg": "2.1.3",
    "mime": "^3.0.0",
    "node-fetch": "^2.6.9",
    "node-webpmux": "3.1.7",
    "pino": "^9.12.0",
    "puppeteer": "^18.2.1",
    "qrcode": "^1.5.4",
    "sonner": "^2.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/node-fetch": "^2.5.12",
    "@types/qrcode": "^1.5.5",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.4.1",
    "eslint-plugin-mocha": "^10.0.3",
    "jsdoc": "^4.0.4",
    "jsdoc-baseline": "^0.1.5",
    "mocha": "^9.0.2",
    "prisma": "^6.16.2",
    "sinon": "^13.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "optionalDependencies": {
    "archiver": "^5.3.1",
    "fs-extra": "^10.1.0",
    "unzipper": "^0.10.11"
  }
}
