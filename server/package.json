{
  "name": "whatsapp-automation-server",
  "version": "1.0.0",
  "description": "WhatsApp Automation Backend Service",
  "main": "app/dist/main.js",
  "scripts": {
    "dev": "tsx --require dotenv/config app/src/main.ts",
    "start": "node app/dist/main.js",
    "build": "tsc --project tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:push": "prisma db push",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "whatsapp",
    "automation",
    "api",
    "backend"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/multipart": "^9.2.1",
    "@fastify/websocket": "^11.2.0",
    "@hapi/boom": "^10.0.1",
    "@prisma/client": "^6.16.2",
    "dotenv": "^16.6.1",
    "fastify": "^5.6.1",
    "fluent-ffmpeg": "2.1.3",
    "mime": "^3.0.0",
    "node-fetch": "^2.6.9",
    "node-webpmux": "3.1.7",
    "pino": "^9.13.1",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "@wppconnect-team/wppconnect": "^1.30.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/node-fetch": "^2.5.12",
    "@types/qrcode": "^1.5.5",
    "@types/ws": "^8.18.1",
    "eslint": "^8.4.1",
    "prisma": "^6.16.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "optionalDependencies": {
    "archiver": "^5.3.1",
    "fs-extra": "^10.1.0",
    "unzipper": "^0.10.11"
  }
}
